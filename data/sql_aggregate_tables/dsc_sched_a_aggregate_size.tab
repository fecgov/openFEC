  OIDS=FALSE
);
ALTER TABLE disclosure.dsc_sched_a_aggregate_size
  OWNER TO fec;
GRANT ALL ON TABLE disclosure.dsc_sched_a_aggregate_size TO fec;
GRANT SELECT ON TABLE disclosure.dsc_sched_a_aggregate_size TO fec_read;

